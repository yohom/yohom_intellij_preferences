<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="Kotlin">
        <added pattern="common.extensions.replaceBatch(sourcesAndDestination)" />
        <added pattern="common.model.Variable(type,name)" />
        <added pattern="kotlin.collections.joinToString(separator,prefix,postfix,limit,truncated,transform)" />
        <added pattern="kotlin.text.replace(oldValue,newValue,ignoreCase)" />
        <added pattern="common.extensions.replaceParagraph(oldValue,newValue)" />
        <added pattern="me.yohom.fluttify.common.extensions.replaceParagraph(oldValue,newValue)" />
        <added pattern="kotlin.text.contains(other,ignoreCase)" />
        <added pattern="me.yohom.fluttify.extensions.mustTrue(message,subject)" />
        <added pattern="me.yohom.fluttify.extensions.mustFalse(message,subject)" />
        <added pattern="me.yohom.fluttify.extensions.mustTrue(message,mapper)" />
        <added pattern="me.yohom.fluttify.extensions.mustFalse(message,mapper)" />
        <added pattern="me.yohom.fluttify.extensions.must(message,mapper)" />
        <added pattern="me.yohom.fluttify.extensions.mustNot(message,mapper)" />
        <added pattern="me.yohom.fluttify.extensions.replaceParagraph(oldValue,newValue)" />
        <added pattern="java.lang.ProcessBuilder.command(command)" />
        <added pattern="kotlin.text.Regex(pattern)" />
        <added pattern="me.yohom.fluttify.extensions.joinToStringX(separator,prefix,suffix,transform)" />
        <added pattern="kotlin.text.split(delimiters,ignoreCase,limit)" />
        <added pattern="java.util.Random.ints(streamSize,randomNumberOrigin,randomNumberBound)" />
        <added pattern="org.gradle.kotlin.dsl.implementation(dependencyNotation)" />
        <added pattern="org.gradle.kotlin.dsl.developmentOnly(dependencyNotation)" />
        <added pattern="org.gradle.kotlin.dsl.annotationProcessor(dependencyNotation)" />
        <added pattern="org.gradle.kotlin.dsl.testImplementation(dependencyNotation)" />
        <added pattern="java.time.Instant.minusSeconds(secondsToSubtract)" />
        <added pattern="java.time.Instant.plusSeconds(secondsToAdd)" />
        <added pattern="org.springframework.data.domain.PageRequest.of(page,size,direction,properties)" />
        <added pattern="kotlin.math.pow(x)" />
        <added pattern="kotlin.math.sqrt(x)" />
        <added pattern="kotlin.math.sin(x)" />
        <added pattern="com.mapbox.geojson.Point.fromLngLat(longitude,latitude)" />
        <added pattern="me.yohom.fluttify.extensions.isOneOf(candidates)" />
        <added pattern="me.yohom.fluttify.extensions.contentOf(name)" />
        <added pattern="me.yohom.fluttify.extensions.invoke(name)" />
        <added pattern="me.yohom.fluttify.extensions.contentOf(node,name)" />
        <added pattern="me.yohom.fluttify.extensions.listBy(name)" />
        <added pattern="kotlin.text.indexOf(string,startIndex,ignoreCase)" />
      </blacklist>
    </blacklists>
    <option id="SHOW_PROPERTY_HINT" value="true" />
    <option id="SHOW_FUNCTION_HINT" value="true" />
    <option id="SHOW_LOCAL_VARIABLE_HINT" value="true" />
  </component>
</application>